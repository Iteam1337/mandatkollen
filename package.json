    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-modern-browsers": "^9.0.2",
    "babel-preset-react-app": "^3.0.1",
