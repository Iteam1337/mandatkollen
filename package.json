{
  "name": "mandatkollen",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "homepage": "https://mandatkollen.se",
  "type": "module",
  "dependencies": {
    "apicache": "^1.6.3",
    "cheerio": "^1.0.0-rc.12",
    "csv-parse": "^5.3.4",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3",
    "mobile-drag-drop": "^3.0.0-rc.0",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.0",
    "object-assign": "^4.1.1",
    "parliament-svg": "^3.0",
    "pureact": "^1.3.0",
    "roboto-fontface": "^0.10.0",
    "typeface-miriam-libre": "^1.1.13",
    "unzip-stream": "^0.3.1",
    "vite": "^4.1.1"
  },
  "scripts": {
    "start": "vite serve",
    "build": "vite build",
    "test": "npx jest scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
